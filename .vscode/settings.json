{
  "[jsonc]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[yaml]": {
    "editor.defaultFormatter": "esbenp.prettier-vscode"
  },
  "[typescript]": {
    "editor.defaultFormatter": "dprint.dprint"
  },
  "deno.codeLens.testArgs": ["--no-check=remote", "-A", "-L=info"],
  "deno.config": "./deno.jsonc",
  "deno.enable": true,
  "deno.lint": true,
  "editor.defaultFormatter": "dprint.dprint",
  "editor.formatOnSave": true,
  "editor.tabSize": 2,
  "prettier.printWidth": 100
}
